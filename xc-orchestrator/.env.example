# XCM Orchestrator Configuration
NODE_ENV=development

# WebSocket Endpoints
POLKADOT_WS_ENDPOINT=wss://rpc.polkadot.io
KUSAMA_WS_ENDPOINT=wss://kusama-rpc.polkadot.io
ASSETHUB_POLKADOT_WS_ENDPOINT=wss://polkadot-asset-hub-rpc.polkadot.io
ASSETHUB_KUSAMA_WS_ENDPOINT=wss://kusama-asset-hub-rpc.polkadot.io
HYDRATION_WS_ENDPOINT=wss://rpc.hydradx.cloud
MOONBEAM_WS_ENDPOINT=wss://wss.api.moonbeam.network
ASTAR_WS_ENDPOINT=wss://rpc.astar.network

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration
PORT=3000
HOST=localhost

# Monitoring Configuration
ENABLE_REAL_TIME_MONITORING=true
MONITORING_INTERVAL=5000
RETRY_ATTEMPTS=3
RETRY_DELAY=2000

# Fee Configuration
FEE_MULTIPLIER=1.2
MAX_FEE_PERCENTAGE=5
AUTO_FEE_ESTIMATION=true

# Security
API_KEY=your-api-key-here
JWT_SECRET=your-jwt-secret

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100